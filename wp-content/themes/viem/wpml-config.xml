<wpml-config>
    <admin-texts>
        <key name="dt_theme_viem">
            <key name="blog-loadmore-text" />
            <key name="advertisement_title" />
            <key name="skip_advertisement_text" />
            <key name="skip_ad_text" />
            <key name="play_btn_tooltip_txt" />
            <key name="pause_btn_tooltip_txt" />
            <key name="rewind_btn_tooltip_txt" />
            <key name="quality_btn_opened_tooltip_txt" />
            <key name="quality_btn_close_tooltip_txt" />
            <key name="mute_btn_tooltip_txt" />
            <key name="unmute_btn_tooltip_txt" />
            <key name="fullscreen_btn_tooltip_txt" />
            <key name="exit_fullscreen_btn_tooltip_txt" />
            <key name="volume_tooltip_txt" />
            <key name="playlist_btn_closed_tooltip_txt" />
            <key name="playlist_btn_opened_tooltip_txt" />
