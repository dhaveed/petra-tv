        </key>
    </admin-texts>   
    <custom-types>
        <custom-type translate="1">viem_video</custom-type>
        <custom-type translate="1">viem_playlist</custom-type>
        <custom-type translate="1">viem_channel</custom-type>
        <custom-type translate="1">viem_series</custom-type>
    </custom-types>
    <taxonomies>
        <taxonomy translate="1">video_cat</taxonomy>
        <taxonomy translate="1">video_tag</taxonomy>
        <taxonomy translate="1">video_badges</taxonomy>
    </taxonomies>
    <shortcodes>
        <shortcode>
            <tag>dt_blog</tag>
            <attributes>
                <attribute>title</attribute>
                <attribute>loadmore_text</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>dt_button</tag>
            <attributes>
                <attribute>text</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>dt_banner</tag>
            <attributes>
                <attribute>title</attribute>
                <attribute>desc</attribute>
            </attributes>
        </shortcode>
        <shortcode>
